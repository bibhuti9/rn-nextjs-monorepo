{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@api/*": ["./src/api/*"],
      "@store/*": ["./src/store/*"],
      "@assets/*": ["./src/assets/*"],
      "@component/*": ["./src/component/*"],
      "@navigation/*": ["./src/navigation/*"],
      "@navigators/*": ["./src/navigators/*"],
      "@screens/*": ["./src/screens/*"],
      "@theme/*": ["./src/theme/*"],
      "@utils/*": ["./src/utils/*"]
    }
  }
}
